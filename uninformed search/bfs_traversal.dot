graph G {
  node [shape=circle];
  0 -- 1 [color=gray];
  0 -- 2 [color=gray];
  1 -- 3 [color=gray];
  2 -- 4 [color=gray];
  3 -- 4 [color=gray];
  4 -- 5 [color=gray];
  0 -- 1 [color=red, penwidth=2, label="1"];
  0 -- 2 [color=red, penwidth=2, label="2"];
  1 -- 3 [color=red, penwidth=2, label="3"];
  2 -- 4 [color=red, penwidth=2, label="4"];
  4 -- 5 [color=red, penwidth=2, label="5"];
}
