graph G {
  node [shape=circle];
  0 -- 1 [color=gray];
  0 -- 3 [color=gray];
  0 -- 4 [color=gray];
  1 -- 2 [color=gray];
  1 -- 5 [color=gray];
  2 -- 5 [color=gray];
  4 -- 5 [color=gray];
  0 -- 1 [color=red, penwidth=2, label="1"];
  0 -- 3 [color=red, penwidth=2, label="2"];
  0 -- 4 [color=red, penwidth=2, label="3"];
  1 -- 2 [color=red, penwidth=2, label="4"];
  1 -- 5 [color=red, penwidth=2, label="5"];
}
